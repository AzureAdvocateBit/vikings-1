{
  "name": "vikings",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-ng": "ng serve -o",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gen-stats": "ng build --prod --stats-json",
    "view-stats": "webpack-bundle-analyzer dist/stats.json",
    "docker-up": "docker-compose up -d --build",
    "docker-up-debug": "docker-compose -f docker-compose.debug.yml up -d --build",
    "docker-down": "docker-compose down",
    "dev-proxy": "concurrently \"npm run debug\" \"ng serve --proxy-config proxy.conf.json --open\"",
    "start": "node server.js",
    "start_debug": "node --inspect=0.0.0.0:9229 server.js",
    "start_azure_debug": "if [[ ${APPSVC_TUNNEL_PORT} != \"\" ]]; then node --inspect=0.0.0.0:$APPSVC_TUNNEL_PORT server.js; else npm run start; fi",
    "local": "WWW=./dist node -r dotenv/config server.js",
    "debug": "WWW=./dist node -r dotenv/config --inspect=9229 server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.2",
    "@angular/cdk": "7.0.2",
    "@angular/common": "7.0.2",
    "@angular/compiler": "7.0.2",
    "@angular/core": "7.0.2",
    "@angular/forms": "7.0.2",
    "@angular/http": "7.0.2",
    "@angular/material": "7.0.2",
    "@angular/platform-browser": "7.0.2",
    "@angular/platform-browser-dynamic": "7.0.2",
    "@angular/router": "7.0.2",
    "@azure/cosmos": "^2.0.4",
    "angular-in-memory-web-api": "^0.7.0",
    "body-parser": "^1.18.2",
    "core-js": "^2.5.4",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "mongodb": "^3.1.8",
    "mongoose": "^5.3.9",
    "rxjs": "6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "7.0.4",
    "@angular/compiler-cli": "7.0.2",
    "@angular/language-service": "7.0.2",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "^10.12.2",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.0.1",
    "dotenv": "^6.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
