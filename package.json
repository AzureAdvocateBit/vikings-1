{
  "name": "vikings",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-ng": "ng serve -o",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gen-stats": "ng build --prod --stats-json",
    "view-stats": "webpack-bundle-analyzer dist/publicweb/stats.json",
    "docker-up": "docker-compose up -d --build",
    "docker-up-debug": "docker-compose -f docker-compose.debug.yml up -d --build",
    "docker-down": "docker-compose down",
    "dev-proxy": "concurrently \"npm run debug\" \"ng serve --proxy-config proxy.conf.json --open\"",
    "start": "node index.js",
    "local": "PUBLICWEB=./dist/publicweb node -r dotenv/config src/server/index.js",
    "debug": "PUBLICWEB=./dist/publicweb node -r dotenv/config --inspect src/server/index.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "angular-in-memory-web-api": "^0.5.4",
    "body-parser": "^1.18.2",
    "core-js": "^2.5.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "mongoose": "^5.0.12",
    "rxjs": "^5.5.8",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.103",
    "codelyzer": "^4.2.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "webpack-bundle-analyzer": "^2.11.1"
  }
